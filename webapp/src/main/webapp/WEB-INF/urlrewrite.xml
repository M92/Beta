<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite default-match-type="wildcard">

    <!-- Rewrites for Angular -->

    <rule>
        <from>/test-page</from>
        <to>/index.html</to>
    </rule>
    <rule>
        <from>/test-page/**</from>
        <to>/index.html</to>
    </rule>

</urlrewrite>
